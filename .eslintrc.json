{
    "parser": "@babel/eslint-parser",
    "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
    "parserOptions": {
        "requireConfigFile": false,
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error"
    }
}
